{
    "name": "theriftlab/immanuel-chart",
    "description": "Laravel package providing a PHP bridge to a Python astrology script powered by Riftlib.",
    "type": "library",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "The Rift Lab",
            "email": "scene@theriftlab.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RiftLab\\ImmanuelChart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiftLab\\ImmanuelChart\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "orchestra/testbench": "^6.11",
        "dms/phpunit-arraysubset-asserts": "^0.2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
